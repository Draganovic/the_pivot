<div class="container">
  <div class="row">
    <div class="page-header">
      <h1>My Trips</h1>
    </div>

    <div class="trips">
      <% @trips.each do |trip| %>
        <div class="panel panel-default">
          <div class="panel-heading">
            <h3 class="panel-title">Trip <small>with 3 locations</small></h3>
          </div>
          <div class="panel-body">
            <div class="trip-info">
              <p>Some Content</p>
            </div>
          </div>
        </div>
      <% end %>
    </div>

  </div>
</div>
