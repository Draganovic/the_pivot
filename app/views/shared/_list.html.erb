<% coder.each do |coder| %>
  <div class="one-quarter">
    <div class="percent-90">
      <p><%= link_to coder.name, coder_path(coder.id), class: "link"%></p>
      <img src="<%= coder.image_url %>" width="100%">
      <%= render partial: attributes, locals: { coder: coder } %>
    </div>
  </div>
<% end %>
